<div class="col-lg-8">
  
  <div class="row">
    
    <div class="col-lg-8">
      <h3>Events</h3>
    </div>
    
    <div class="col-lg-4">
      <br>
      <div class="btn-group pull-right">
        <button type="button" class="btn btn-sm btn-default dropdown-toggle" data-toggle="dropdown">
          <span class="glyphicon glyphicon-plus"></span> Event <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" role="menu">
          <% Verb.all.each do |v| %>
            <li><%= link_to v.display, new_event_path(artifact_id: @artifact.uuid, type: v.keyword) %></li>
          <% end %>
        </ul>
      </div>
    </div>
    
  </div>
  

  <table class="table table-striped">
    <tr>
      <td><h4>1970</h4></td>
      <td>
        <div class="row">
          <div class="col-lg-12">
            <strong>Discovered by</strong> <span><a href="#"><img data-src="holder.js/30x30"> Bob Smith</a></span> at <span><a href="#"><img data-src="holder.js/30x30"> Florence, Italy</a></span>
          </div>
        </div>
      </td>
    </tr>
    <tr>
      <td><h4>1973</h4></td>
      <td>
        <div class="row">
          <div class="col-lg-12">
            <strong>Sold to</strong> <span><a href="#"><img data-src="holder.js/30x30"> The Getty Villa</a></span>, by <span><a href="#"><img data-src="holder.js/30x30"> Gianfranco Bechinna</a></span>
          </div>
        </div>
      </td>
    </tr>
  </table>
</div>